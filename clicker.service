[Unit]
Description=Test app
After=syslog.target network.target

[Service]
SuccessExitStatus=143

User=root
Group=root

Type=simple

Environment="JAVA_HOME=/usr/bin/java"
WorkingDirectory=/app/clicker-main
ExecStart=/bin/java -jar target/Clicker-0.0.1-SNAPSHOT.jar

Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target